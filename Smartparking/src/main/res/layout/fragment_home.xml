<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <ImageView
        android:id="@+id/book_imagebtn"
        android:layout_width="145dp"
        android:layout_height="124dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="156dp"
        android:src="@drawable/book1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/support_imagebtn"
        android:layout_width="138dp"
        android:layout_height="150dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="396dp"
        android:src="@drawable/support"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/feedback_imagebtn"
        android:layout_width="140dp"
        android:layout_height="128dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="156dp"
        android:src="@drawable/feedback"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/share_imagebtn"
        android:layout_width="134dp"
        android:layout_height="140dp"
        android:layout_marginTop="404dp"
        android:src="@drawable/share_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="127dp"
        android:layout_height="93dp"
        android:layout_marginStart="244dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/parkingcar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/book_home"
        android:layout_width="119dp"
        android:layout_height="43dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="304dp"
        android:text="@string/book_space"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/share_home"
        android:layout_width="88dp"
        android:layout_height="0dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="580dp"
        android:text="@string/share_info"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/support_home"
        android:layout_width="112dp"
        android:layout_height="34dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="580dp"
        android:text="@string/support_center"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/feedback_home"
        android:layout_width="144dp"
        android:layout_height="48dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="304dp"
        android:text="@string/feedback"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="183dp"
        android:layout_height="52dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:text="@string/dashboard"
        android:textSize="29sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_btn"
        android:layout_width="wrap_content"
        android:layout_height="64dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="612dp"
        android:src="@drawable/ic_baseline_logout_24"
        app:fabSize="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>